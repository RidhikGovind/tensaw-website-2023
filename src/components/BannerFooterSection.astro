</style>

<!-- CTA Banner -->
<div class="relative overflow-hidden">
	<div
		class="max-w-[85rem] md:w-full px-4 py-12 lg:pt-0 sm:px-6 lg:px-16 lg:py-16 mx-auto rounded-md"
	>
		<!-- Grid -->
		<div
			class="rounded-md banner-ctaContainer w-auto p-8 py-10"
			style="height:32rem;"
		>
			<div class="z-20 absolute banner-ctaContainer--content">
				<!-- Title -->
				<div class="mt-5 max-w-xl lg:max-w-2xl text-center mx-auto">
					<h1 class="block font-bold text-white text-4xl md:text-5xl">
						Patient tools to help you grow your practice
					</h1>
				</div>
				<!-- End Title -->

				<div class="mt-5 max-w-sm text-center mx-auto">
					<p class="md:text-lg text-white">
						Empower Your Practice with Effective Patient Tools for Sustainable
						Growth
					</p>
				</div>

				<button
					type="button"
					class="py-3 px-4 mt-6 inline-flex calendly-open-button justify-center items-center gap-2 font-medium rounded-md border border-transparent bg-[#FD3E81] text-white hover:bg-[#E43975] focus:outline-none focus:ring-2 focus:ring-[#E43975] focus:ring-offset-2 transition-all dark:focus:ring-offset-gray-800"
				>
					<a onclick="openCalendlyWidget()" href="#calendly"> Get Started</a>
				</button>
			</div>

			<div class="banner-ctaContainer--Overlay"></div>
		</div>
		<!-- End Grid -->
	</div>
</div>
