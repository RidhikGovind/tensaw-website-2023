<style>
	/* ------------ CALENDLY WIDGET STYLES ------------------- */
	.calendly-widget-overlay {
		position: fixed;
		display: none;
		width: 100%;
		height: 100%;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		background-color: rgba(0, 0, 0, 0.5);
		z-index: 9999;
		cursor: pointer;
	}

	.calendly-inline-widget {
		position: absolute;
		width: 100%;
		height: 100%;
		z-index: 9999;
		/* display: none; */
	}

	.calendly-widget-close {
		z-index: 999999;
		padding: 2rem;
		color: #fff;
		position: absolute;
		top: 0;
		right: 0;
	}

	.testimonial-container {
		background: no-repeat url('/images/wave-pink.svg');
		background-size: contain;
		background-position: bottom right;
		background-color: #ffecf7;
	}

	@media (max-width: 520px) {
		.testimonial-container {
			background-position: bottom -40px right -20px;
		}
	}
</style>

